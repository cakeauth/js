{
  "name": "@cakeauth/react",
  "version": "0.1.4",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/lib/index.d.ts",
  "style": "dist/styles.css",
  "exports": {
    "./styles": "./dist/styles.css",
    ".": {
      "types": "./dist/lib/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./hooks": {
      "types": "./dist/lib/hooks/index.d.ts",
      "import": "./dist/hooks/index.js",
      "require": "./dist/hooks/index.cjs",
      "default": "./dist/hooks/index.js"
    },
    "./components": {
      "types": "./dist/lib/components/index.d.ts",
      "import": "./dist/components/index.js",
      "require": "./dist/components/index.cjs",
      "default": "./dist/components/index.js"
    },
    "./functions": {
      "types": "./dist/lib/functions/index.d.ts",
      "import": "./dist/functions/index.js",
      "require": "./dist/functions/index.cjs",
      "default": "./dist/functions/index.js"
    }
  },
  "sideEffects": [
    "**/*.css",
    "dist/**/*.js",
    "**/*/client.*",
    "**/*/hooks.*"
  ],
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/cakeauth/js"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b ./tsconfig.lib.json && vite build",
    "release": "npm run build && standard-version && git push --follow-tags && npm publish",
    "clean": "rm -rf .turbo node_modules dist examples/vite/node_modules/ examples/base/vite/ examples/vite/.next/"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": ">=3.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    },
    "react-dom": {
      "optional": false
    }
  },
  "keywords": [
    "cakeauth"
  ],
  "author": "CakeAuth",
  "license": "MIT",
  "dependencies": {
    "@cakeauth/frontend": "workspace:*",
    "@radix-ui/react-avatar": "^1.1.2",
    "@radix-ui/react-checkbox": "^1.1.3",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-dropdown-menu": "^2.1.4",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-popover": "^1.1.4",
    "@radix-ui/react-scroll-area": "^1.2.2",
    "@radix-ui/react-separator": "^1.1.1",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-tabs": "^1.1.2",
    "@tanstack/react-query": "^5.63.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.4",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.469.0",
    "react-cookie": "^7.2.2",
    "react-google-recaptcha": "^3.1.0",
    "react-phone-number-input": "^3.4.10",
    "react-turnstile": "^1.1.4",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/node": "^22.10.5",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-google-recaptcha": "^2.1.9",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "glob": "^11.0.0",
    "postcss": "^8.4.49",
    "standard-version": "^9.5.0",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5",
    "vite-plugin-dts": "^4.4.0",
    "vite-plugin-lib-inject-css": "^2.2.1"
  }
}
